,IAAAA,WAAW,CAACV,KAAD,EAAQQ,IAAR,EAAc;AACrBR,MAAAA,KAAK,CAACE,IAAN,CAAWS,IAAX,CAAgBH,IAAhB;AACAI,MAAAA,OAAO,CAACC,GAAR,CAAYL,IAAZ;AAEH;;AAdU;AALgB,CAAf,CAAd;AAuBA,MAAMM,MAAM,GAAG,CACb;AAAEC,EAAAA,IAAI,EAAE,QAAR;AAAkBC,EAAAA,IAAI,EAAE,OAAxB;AAAiCC,EAAAA,SAAS,EAAE1B;AAA5C,CADa,EAEb;AAAEwB,EAAAA,IAAI,EAAE,GAAR;AAAaC,EAAAA,IAAI,EAAE,MAAnB;AAA2BC,EAAAA,SAAS,EAAE5B;AAAtC,CAFa,EAGb;AAAE0B,EAAAA,IAAI,EAAE,UAAR;AAAoBC,EAAAA,IAAI,EAAE,SAA1B;AAAqCC,EAAAA,SAAS,EAAE7B;AAAhD,CAHa,EAIb;AAAE2B,EAAAA,IAAI,EAAE,UAAR;AAAoBC,EAAAA,IAAI,EAAE,SAA1B;AAAqCC,EAAAA,SAAS,EAAE3B;AAAhD,CAJa,EAKb;AAAEyB,EAAAA,IAAI,EAAE,OAAR;AAAiBC,EAAAA,IAAI,EAAE,MAAvB;AAA+BC,EAAAA,SAAS,EAAEzB;AAA1C,CALa,CAAf;AAQA,MAAM0B,MAAM,GAAG,IAAI/B,SAAJ,CAAc;AAC3B2B,EAAAA;AAD2B,CAAd,CAAf;AAIA,IAAI5B,GAAJ,CAAQ;AACNgC,EAAAA,MADM;AAENpB,EAAAA,KAFM;AAGNqB,EAAAA,EAAE,EAAE,MAHE;AAINC,EAAAA,MAAM,EAAEC,CAAC,IAAIA,CAAC,CAACC,OAAO,CAAC,WAAD,CAAR;AAJR,CAAR","sourcesContent":["import Vue from \"vue\";\n//import App from \"./App.vue\";\nimport VueRouter from \"vue-router\";\nimport Product from \"./components/Product.vue\";\nimport Home from \"./components/Home.vue\";\nimport Contact from \"./components/Contact.vue\";\nimport About from \"./components/About.vue\";\nimport Cart from \"./components/Cart.vue\";\nimport Vuex from \"vuex\"\nimport axios from \"axios\";\n\n\nVue.use(Vuex)\n\n\nVue.config.productionTip = false;\n\nVue.use(VueRouter);\n\nconst store = new Vuex.Store({\n  state: {\n    products: [],\n    cart: [],\n  },\n  mutations: {\n    GET_PRODUCT(state) {\n      axios.get(\"https://fakestoreapi.com/products\").then(\n          function (response) {\n              state.products = response.data;\n              //console.log(response.data);\n          }.bind(this)\n      );\n\n  },\n  ADD_TO_CART(state, data) {\n      state.cart.push(data);\n      console.log(data);\n\n  }\n  }\n})\n\nconst routes = [\n  { path: \"/About\", name: 'About', component: About },\n  { path: \"/\", name: 'Home', component: Home },\n  { path: \"/Product\", name: 'Product', component: Product },\n  { path: \"/Contact\", name: 'Contact', component: Contact },\n  { path: \"/Cart\", name: 'Cart', component: Cart }\n];\n\nconst router = new VueRouter({\n  routes\n});\n\nnew Vue({\n  router,\n  store,\n  el: '#app',\n  render: h => h(require('./App.vue'))\n})\n"]}]}